	<Container File="Project.proj" TargetFileName=".bhproj/{{projectname}}.ksproj" ReplaceParameters="true">
	    <Item SubType="" CustomTool="" ItemType="" ReplaceParameters="false" TargetFileName="">
		index.html
	    </Item>
	    <Folder Name="app">
	        <Item SubType="" CustomTool="" ItemType="" ReplaceParameters="false" TargetFileName="">index.html</Item>
	    </Folder>
	    <Folder Name="core">
		<Folder Name="cfg">
		    <Item SubType="" CustomTool="" ItemType="" ReplaceParameters="false" TargetFileName="config.xml">config.xml</Item>
		</Folder>
	    </Folder>
	</Container>
    </TemplateContent>
