<?xml version='1.0' encoding='utf-8'?>
<BHTemplate Type='Item' Version='0.3'>
    <TemplateData>
        <Name>BHPanel</Name>
        <Description>A couple of Ksike's JavaScript classes definition, that BHike uses as View-Controller classes for the client side implementation. These two classes are only for BHike development at this moment.</Description>
        <Type>Ksike Items</Type>
        <SubType>BHike</SubType>
        <SortOrder>100</SortOrder>
        <DefaultName>BHPanel</DefaultName>
        <ProvideDefaultName>false</ProvideDefaultName>
    </TemplateData>
    <TemplateContent>
		<Item ReplaceParameters='true' TargetFileName='view.js.tpl'>
			<Value>{$GLOBALS['name']}.js</Value>
		</Item>
		<Item ReplaceParameters='true' TargetFileName='class.js.tpl'>
			<Value>{$GLOBALS['name']}.class.js</Value>
		</Item>
    </TemplateContent>
</BHTemplate>
